---
import Base from "../layouts/Base.astro";
---
<Base title="Not Found" description="404">
  <section class="hero">
    <h1 style="font-size:34px;">404</h1>
    <p>This path does not exist in the current static build. Start from <a href="/explore">Explore</a>.</p>
    <div class="cta-row">
      <a class="btn primary" href="/explore">Explore</a>
      <a class="btn" href="/publish">Publish</a>
    </div>
  </section>
</Base>

